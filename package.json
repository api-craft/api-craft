{
  "name": "@tselven/api-craft",
  "version": "1.2.1",
  "description": "A Framework for build APIs using Express + Mongoose",
  "main": "server.js",
  "bin": {
    "api-craft": "./bin/api-craft.js"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "dev": "nodemon server.js --ext js,yaml",
    "start": "node server.js"
  },
  "type": "module",
  "keywords": [
    "express",
    "API",
    "mongoose",
    "backend",
    "Framework",
    "auth"
  ],
  "author": "P.Thamilselven <contact@tselven.com>",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/api-craft/api-craft.git"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.12.1",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.2",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "babel-jest": "^30.0.0",
    "cross-env": "^7.0.3",
    "jest": "^30.0.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "bcrypt"
    ]
  }
}
